    "def print_permutation(s,target):\n",
    "    if (len(s)==0):\n",
    "        print(target)\n",
    "        return \n",
    "    first_char = s[0]\n",
    "    next_char = s[1:]\n",
    "    for i in range(0,len(target)+1):\n",
    "        print_permutation(next_char,target[0:i]+first_char+target[i:])\n",
    "\n",
    "\n",
    "print(print_permutation(\"abc\",''))"
   ]
