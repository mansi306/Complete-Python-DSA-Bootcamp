    "def update_list(l1,n,index,anslist):\n",
    "\n",
    "    if index == len(l1):\n",
    "        return \n",
    "    if (l1[index]==n):\n",
    "        anslist.append(index)\n",
    "    \n",
    "    update_list(l1,n,index+1,anslist)\n",
    "anslist = []\n",
    "\n",
    "update_list([1,2,3,4,5,3,2,1],2,0,anslist)\n",
    "print(anslist)"
   ]
